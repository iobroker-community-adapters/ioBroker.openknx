    "scripts": {
        "test:js": "mocha --exit --reporter-option maxDiffSize=50000 --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
        "test:package": "mocha test/package --exit",
